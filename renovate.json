{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":disableDependencyDashboard", ":enablePreCommit"],
  "packageRules": [
    {
      "matchUpdateTypes": ["digest", "minor", "patch", "pin"],
      "automerge": true
    }
  ],
  "autoApprove": true,
  "platformAutomerge": true
}
